{
  "name": "footballmanager",
  "version": "1.0.0",
  "description": "API services to manage premier league football database",
  "main": "index.js",
  "scripts": {
    "prestart": "npm run-script lint",
    "start": "ts-node src/index.ts",
    "start:dev": "./node_modules/.bin/nodemon",
    "startNaive": "tsc && node dist/index.js",
    "build": "tsc -p .",
    "lint": "tsc --noEmit && eslint ./**/*.ts --quiet --fix",
    "test": "jest"
  },
  "keywords": [
